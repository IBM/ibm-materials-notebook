{
  "name": "@ibm-materials/cmdl",
  "version": "0.0.1",
  "description": "CMDL language package",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "chevrotain": "^10.3.0",
    "@ibm-materials/cmdl-types": "*",
    "@ibm-materials/cmdl-chemicals": "*",
    "@ibm-materials/cmdl-polymers": "*",
    "@ibm-materials/cmdl-reactors": "*",
    "@ibm-materials/cmdl-units": "*",
    "@ibm-materials/logger": "*"
  },
  "devDependencies": {
    "@ibm-materials/eslint-config": "*",
    "@ibm-materials/jest-config": "*",
    "@ibm-materials/ts-config": "*"
  },
  "files": [
    "LISCENSE.md",
    "dist/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./dist",
    "prepublish": "npm run clean && npm run build",
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "lint": "eslint src --ext ts --fix",
    "test": "jest",
    "generate-types": "npx ts-node ./src/parser/language-type-generator.ts"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/ibm-materials-notebook"
  },
  "author": "npark",
  "license": "MIT"
}
